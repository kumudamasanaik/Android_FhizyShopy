<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <!-- Common Styles -->
    <style name="ToolbarStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="navigationIcon">@drawable/ic_menu_</item>
        <item name="layout_scrollFlags">scroll|enterAlways</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    </style>

    <style name="ToolbarTitleStyle">
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:text">@string/tool_bar_tittle</item>
        <item name="android:textColor">@color/md_white_1000</item>
        <item name="android:textSize">@dimen/medium</item>
    </style>


    <style name="MyActionButtonOverflow" parent="android:style/Widget.Holo.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_menu_</item>
        <item name="android:maxHeight">@dimen/dimens_45</item>
    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@color/md_white_1000</item>
    </style>

    <style name="MyActionBar" parent="@style/Theme.AppCompat.Light">
        <item name="android:popupBackground">@color/md_white_1000</item>
    </style>

    <style name="login_edit_text">
        <item name="android:layout_width">263dp</item>
        <item name="android:layout_height">51dp</item>
        <item name="android:background">@drawable/light_red_rect_border</item>
        <item name="android:textColorHint">@color/md_grey_400</item>
        <item name="android:textColor">@color/app_text_black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:ems">10</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="login_btn" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:layout_width">254dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/red_rect</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textColor">@color/app_text_white</item>
    </style>

    <style name="AppTheme_Loading_Dialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="windowNoTitle">true</item>
        <item name="android:background">@color/colorTransparent</item>
        <item name="android:windowBackground">@color/md_white_1000</item>
        <item name="windowActionBar">false</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="edit_text_bottom_line" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlNormal">@color/md_grey_300</item>
        <item name="colorControlActivated">@color/md_grey_300</item>
        <item name="colorControlHighlight">@color/md_grey_300</item>
    </style>


    <style name="searchview_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:layout_marginRight">@dimen/dimens_10</item>
        <item name="android:layout_marginLeft">@dimen/dimens_10</item>
        <item name="android:layout_marginBottom">@dimen/dimens_5</item>
        <item name="android:layout_marginTop">@dimen/dimens_5</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    </style>

    <style name="search_editTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimens_30</item>
        <item name="android:hint">@string/search_hint</item>
        <item name="android:layout_marginRight">@dimen/dimens_10</item>
        <item name="android:layout_marginLeft">@dimen/dimens_10</item>
