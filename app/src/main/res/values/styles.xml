        <item name="android:textSize">@dimen/small</item>
        <item name="android:textColor">@color/app_text_black</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:layout_marginTop">@dimen/dimens_5</item>

    </style>

    <style name="search_iconStyle">
        <item name="android:layout_width">@dimen/dimens_20</item>
        <item name="android:layout_height">@dimen/dimens_20</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_marginLeft">@dimen/dimens_10</item>
        <item name="android:layout_marginTop">@dimen/dimens_1</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    </style>

    <style name="menu_drawer_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:maxLines">1</item>
        <item name="android:text">@string/app_name</item>
        <item name="android:textColor">@color/app_text_black</item>
        <item name="android:textSize">@dimen/small</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
    </style>

    <style name="menu_drawer_icon">
        <item name="android:layout_width">@dimen/dimens_15</item>
        <item name="android:layout_height">@dimen/dimens_15</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/dimens_15</item>
        <item name="android:maxLines">1</item>
        <item name="android:text">@string/app_name</item>
        <item name="android:src">@drawable/svg_nav_aboutus</item>
    </style>

    <style name="menu_drawer_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimens_0.5</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/dimens_0</item>
        <item name="android:layout_marginTop">@dimen/dimens_10</item>
        <item name="android:background">@color/colorC3CFD1</item>

    </style>

    <style name="login_checkout_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/dimens_10</item>
        <item name="android:background">@drawable/btn_login_to_checkout</item>
        <item name="android:drawablePadding">@dimen/dimens_10</item>
        <item name="android:drawableRight">@drawable/svg_nav_login_arrow_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:padding">@dimen/dimens_10</item>
        <item name="android:text">@string/login_to_checkout</item>
        <item name="android:textColor">@color/app_text_white</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">@dimen/small</item>
    </style>


    <!--MULTIVIEW STYLE -->
    <style name="MultiStateViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="msv_animateViewChanges">false</item>
        <item name="msv_emptyView">@layout/empty_view</item>
        <item name="msv_errorView">@layout/error_view</item>
        <item name="msv_loadingView">@layout/loading_view</item>
        <item name="msv_viewState">content</item>
    </style>


    <style name="SubmitOutlineBtn">
        <item name="android:layout_width">@dimen/submit_btn_width</item>
        <item name="android:layout_height">@dimen/submit_btn_height</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginBottom">@dimen/dimens_10</item>
        <item name="android:layout_marginLeft">@dimen/dimens_8</item>
        <item name="android:layout_marginRight">@dimen/dimens_8</item>
